{
  "name": "@ndragun92/packages-npm-tsup",
  "version": "0.0.0-dev",
  "description": "Tsup Npm Package Template",
  "type": "module",
  "main": "./dist/packages-npm-tsup.cjs",
  "module": "./dist/packages-npm-tsup.js",
  "types": "./dist/packages-npm-tsup.d.ts",
  "exports": {
    ".": {
      "import": "./dist/packages-npm-tsup.js",
      "require": "./dist/packages-npm-tsup.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "lint": "eslint --fix --ext .js,.ts .",
    "pack": "npm pack --dry-run"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ndragun92/packages-npm-tsup.git"
  },
  "keywords": [
    "packages-npm-tsup"
  ],
  "author": "Nemanja Dragun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ndragun92/packages-npm-tsup/issues"
  },
  "homepage": "https://github.com/ndragun92/packages-npm-tsup#readme",
  "publishConfig": {
    "@ndragun92:registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
